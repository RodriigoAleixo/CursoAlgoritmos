algoritmo "semnome"
// 48) Faça um programa que leia 7 números inteiros e no final mostre o somatório
// entre eles.
var
   C, S: Inteiro
inicio
      C <- 1
      S <- 0
      Enquanto (C < 8) faca
      Escreva (C)
      S <- S + C
      C  <- C + 1
      Fimenquanto
      Escreval (S)
fimalgoritmo
